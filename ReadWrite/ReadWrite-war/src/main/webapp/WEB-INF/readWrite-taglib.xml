<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
	version="2.0">
	<namespace>http://ameyakhasgiwala/tags/readWrite-taglib</namespace>
	<tag>
		<tag-name>readWrite</tag-name>
		<component>
			<component-type>readWrite</component-type>
		</component>
		<attribute id="readOnlyOn">
			<name>readOnlyOn</name>
			<required>true</required>
		</attribute>
		<attribute id="skipChildren">
			<name>skipChildren</name>
			<required>false</required>
		</attribute>
	</tag>
</facelet-taglib>